{<br />
&nbsp; int i,j,min,tem,a[N];<br />
&nbsp; /*input data*/<br />
&nbsp; printf(&quot;please input ten num:\n&quot;);<br />
&nbsp; for(i=0;i&lt;N;i++)<br />
&nbsp; {<br />
&nbsp;&nbsp;&nbsp; printf(&quot;a[%d]=&quot;,i);<br />
&nbsp;&nbsp;&nbsp; scanf(&quot;%d&quot;,&amp;a);<br />
&nbsp; }<br />
&nbsp; printf(&quot;\n&quot;);<br />
&nbsp; for(i=0;i&lt;N;i++)<br />
&nbsp;&nbsp;&nbsp; printf(&quot;%5d&quot;,a);<br />
&nbsp; printf(&quot;\n&quot;);<br />
&nbsp; /*sort ten num*/<br />
&nbsp; for(i=0;i&lt;N-1;i++)<br />
&nbsp; {<br />
&nbsp;&nbsp;&nbsp; min=i;<br />
&nbsp;&nbsp;&nbsp; for(j=i+1;j&lt;N;j++)<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; if(a[min]&gt;a[j])<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; min=j;<br />
&nbsp;&nbsp;&nbsp; tem=a;<br />
