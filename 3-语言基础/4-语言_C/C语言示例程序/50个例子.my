<p><br />
#include &quot;stdio.h&quot;<br />
#include &quot;conio.h&quot;<br />
main()<br />
{<br />
&nbsp; int i,j,k,n;<br />
&nbsp; printf(&quot;&#39;water flower&#39;number is:&quot;);<br />
&nbsp; for(n=100;n&lt;1000;n++)<br />
&nbsp; {<br />
&nbsp;&nbsp;&nbsp; i=n/100;/*分解出百位*/<br />
&nbsp;&nbsp;&nbsp; j=n/10%10;/*分解出十位*/<br />
&nbsp;&nbsp;&nbsp; k=n%10;/*分解出个位*/<br />
